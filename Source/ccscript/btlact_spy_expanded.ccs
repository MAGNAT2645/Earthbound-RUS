BTLACT_SPY_UNKNOWN_1:
    LDX_a(0xA972)
    LDA_x(0x0039)
    AND_i(0x00FF)
    CMP_i(0x00FF)
    BNE_a(BTLACT_SPY_UNKNOWN_2)
