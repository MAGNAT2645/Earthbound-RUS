// КОНЕЦ
BTLACT_SPY_PRINT_DESC:
    LDX_a(0xA972) // X <- CURRENT_TARGET
    LDA_x(0x000E) // A <- X[Фраг сторонника (00) или врага (01)]
    AND_i(0x00FF) // A &= #$00FF
    CMP_i(0x0000) // A == #$0000
    BEQ_a(BTLACT_SPY_UNKNOWN_5)

    BText(BTLACT_SPY_DESCRIPTION_TEXT)
    JSL(0xC438B1) // PRINT_NEWLINE

    LDA_i(0x0003)
    STA_a(0x964F) // Костыль, отключающий (03) звук печатания текста

    LDA_i(short[0] ENEMY_DESC_TABLE) // Младшая часть ENEMY_DESC_TABLE
    STA_d(0x06)
    LDA_i(short[1] ENEMY_DESC_TABLE) // Старшая часть ENEMY_DESC_TABLE
    STA_d(0x08)
    LDX_a(0xA972) // X <- CURRENT_TARGET
    LDA_x(0x0000) // A <- X[ID]
    ASL // A <<= 1
    ASL // A <<= 1
    ADC_d(0x06) // ($06 += A) & 0xFF
    STA_d(0x06)
    LDA_dl(0x06)
    STA_d(0x0E)
    INC_d(0x06) // $06++
    INC_d(0x06) // $06++
    LDA_dl(0x06)
    STA_d(0x10)
    JSL(0xC186B1) // DISPLAY_TEXT

    LDA_i(0x0001)
    STA_a(0x964F) // Костыль, возвращающий звук печатания текста в обычный режим (01)
// ОРИГИНАЛЬНАЯ EARTHBOUND
BTLACT_SPY_UNKNOWN_5:
    LDX_a(0xA972)
    LDA_x(0x000E)
    AND_i(0x00FF)
    CMP_i(0x0001)
    BNE_a(BTLACT_SPY_UNKNOWN_6)
    LDA_i(0x0003)
    JSL(0xC4572B) // FIND_INVENTORY_SPACE2
    CMP_i(0x0000)
    BEQ_a(BTLACT_SPY_UNKNOWN_6)
    LDA_a(0xAA10) // ITEM_DROPPED
    BEQ_a(BTLACT_SPY_UNKNOWN_6)
    SEP(0x20) // #PROC_FLAGS::ACCUM8
    LDA_a(0xAA10) // ITEM_DROPPED
    JSL(0xC1DD7C) // REDIRECT_C1ACF8 (A -> $9D11)
    ASMLoadAddress0E(data_63.l_0xef7dd5)
    JSL(0xC1DC1C)
    STZ_a(0xAA10) // ITEM_DROPPED = 0
BTLACT_SPY_UNKNOWN_6:
    PLD
    RTL
// КОНЕЦ
}

